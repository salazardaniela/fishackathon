<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Fishtter</title>
	<link rel="stylesheet" href="../scss/form.css">
</head>
<body>

	<nav>
		<div class="logo">
			<img src="../../../images/FISHTTER ICONS-04.png" alt="">
		</div>
		<div class="title">
			<h1>Form</h1>	
		</div>
			
	</nav>
	
	
	<form>
		<div class="form-element">
			<label>First Name</label>
			<input type="text" name="firstname">
		</div>
				
		<div class="form-element">
			<label>Last Name</label>
			<input type="text" name="lastname">
		</div>
				
		<div class="form-element">
			<label>ID</label>
			<input type="text" name="ID">
		</div>
					
		<div class="form-element">
			<label>Enrollment</label>
			<input type="text" name="Enrollment">	
		</div>
					
		<div class="form-element">
			<label>Port Of Departure</label>
			<input type="text" name="portofdeparture">				
		</div>
					
		<div class="form-element">
			<label>Ship Capacity</label>
			<input type="text" name="shipcapacity">					
		</div>
		
		<div class="form-element">	
			<input type="submit" value="submit">
		<div>
	</form>
	
	<script src="../../../node_modules/jquery/dist/jquery.min.js"></script>
	<script src="../scripts/form.js"></script>
</body>
</html>